<?xml version="1.0" encoding="utf-8"?>
<device schemaVersion="1.1" 
    xmlns:xs="http://www.w3.org/2001/XMLSchema-instance" xs:noNamespaceSchemaLocation="CMSIS-SVD.xsd">
    <name>GBA</name>
    <version>0.5</version>
    <description>Game Boy Advance SVD File for my Louise library </description>
    <addressUnitBits>8</addressUnitBits>
    <width>32</width>
    <resetValue>0x0</resetValue>
    <resetMask>0xFFFFFFFF</resetMask>
    <peripherals>
        <!-- LCD -->
        <peripheral>
            <name>LCD</name>
            <description>LCD I/O Registers</description>
            <baseAddress>0x4000000</baseAddress>
            <addressBlock>
                <offset>0</offset>
                <size>0x56</size>
                <usage>registers</usage>
            </addressBlock>
            <registers>
                <!-- DISPCNT -->
                <register>
                    <name>DISPCNT</name>
                    <description>LCD I/O Display Control</description>
                    <addressOffset>0x0</addressOffset>
                    <size>16</size>
                    <access>read-write</access>
                    <fields>
                        <field>
                            <name>BGMode</name>
                            <description>Background Mode</description>
                            <bitRange>[2:0]</bitRange>
                            <access>read-write</access>
                            <enumeratedValues>
                                <enumeratedValue>
                                    <name>Mode0</name>
                                    <description>  0     No       0123   256x256..512x515   1024  16/16..256/1 SFMABP</description>
                                    <value>0</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>Mode1</name>
                                    <description>  1     Mixed    012-   (BG0,BG1 as above Mode 0, BG2 as below Mode 2)</description>
                                    <value>1</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>Mode2</name>
                                    <description>  2     Yes      --23   128x128..1024x1024 256   256/1        S-MABP</description>
                                    <value>2</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>Mode3</name>
                                    <description>  3     Yes      --2-   240x160            1     32768        --MABP</description>
                                    <value>3</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>Mode4</name>
                                    <description>  4     Yes      --2-   240x160            2     256/1        --MABP</description>
                                    <value>4</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>Mode5</name>
                                    <description>  5     Yes      --2-   160x128            2     32768        --MABP</description>
                                    <value>5</value>
                                </enumeratedValue>
                            </enumeratedValues>
                        </field>
                        <field>
                            <name>CGBMode</name>
                            <description>Check if CGB or AGB mode</description>
                            <bitRange>[3:3]</bitRange>
                            <access>read-only</access>
                        </field>
                        <field>
                            <name>DisplayFrameSelect</name>
                            <description>Display Frame Select for only Background Mode 4/5</description>
                            <bitRange>[4:4]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>HBIntervalFree</name>
                            <description>Allow access to OAM during H-Blank</description>
                            <bitRange>[5:5]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OBJCharVRAMMapping</name>
                            <description>OBJ Character VRAM Mapping</description>
                            <bitRange>[6:6]</bitRange>
                            <access>read-write</access>
                            <enumeratedValues>
                                <enumeratedValue>
                                    <name>TwoDimension</name>
                                    <description>One dimensional array</description>
                                    <value>0</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>OneDimension</name>
                                    <description>Two dimensional array</description>
                                    <value>1</value>
                                </enumeratedValue>
                            </enumeratedValues>
                        </field>
                        <field>
                            <name>ForcedBlank</name>
                            <description>If set to 1, allows FAST access to VRAM, Palette, OAM</description>
                            <bitRange>[7:7]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>DisplayBG0</name>
                            <description>If set to 1, displays Background 0</description>
                            <bitRange>[8:8]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>DisplayBG1</name>
                            <description>If set to 1, displays Background 1</description>
                            <bitRange>[9:9]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>DisplayBG2</name>
                            <description>If set to 1, displays Background 2</description>
                            <bitRange>[10:10]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>DisplayBG3</name>
                            <description>If set to 1, displays Background 3</description>
                            <bitRange>[11:11]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>DisplayOBJ</name>
                            <description>If set to 1, displays Objects</description>
                            <bitRange>[12:12]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>DisplayWindow0</name>
                            <description>If set to 1, displays Window 0</description>
                            <bitRange>[13:13]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>DisplayWindow1</name>
                            <description>If set to 1, displays Window 1</description>
                            <bitRange>[14:14]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>DisplayWindowOBJ</name>
                            <description>If set to 1, displays Window Objects</description>
                            <bitRange>[15:15]</bitRange>
                            <access>read-write</access>
                        </field>
                    </fields>
                </register>
                <!-- DISPSTAT -->
                <register>
                    <name>DISPSTAT</name>
                    <description>LCD I/O Interrupts</description>
                    <addressOffset>0x4</addressOffset>
                    <size>16</size>
                    <access>read-write</access>
                    <fields>
                        <field>
                            <name>VBlankFlag</name>
                            <description>VBlank Status Flag</description>
                            <bitRange>[0:0]</bitRange>
                            <access>read-only</access>
                        </field>
                        <field>
                            <name>HBlankFlag</name>
                            <description>HBlank Status Flag</description>
                            <bitRange>[1:1]</bitRange>
                            <access>read-only</access>
                        </field>
                        <field>
                            <name>VCounterFlag</name>
                            <description>VCounter Status Flag</description>
                            <bitRange>[2:2]</bitRange>
                            <access>read-only</access>
                        </field>
                        <field>
                            <name>VBlankIRQEnable</name>
                            <description>VBlank IRQ Enable Flag</description>
                            <bitRange>[3:3]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>HBlankIRQEnable</name>
                            <description>HBlank IRQ Enable Flag</description>
                            <bitRange>[4:4]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>VCounterIRQEnable</name>
                            <description>VCounter IRQ Enable Flag</description>
                            <bitRange>[5:5]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>VCountSettings</name>
                            <description>The V-Count-Setting value is much the same as LYC of older gameboys, when its value is identical to the content of the VCOUNT register then the V-Counter flag is set (Bit 2), and (if enabled in Bit 5) an interrupt is requested.</description>
                            <bitRange>[15:8]</bitRange>
                            <access>read-write</access>
                        </field>
                    </fields>
                </register>
                <!-- VCOUNT -->
                <register>
                    <name>VCOUNT</name>
                    <description>LCD VCount Status</description>
                    <addressOffset>0x6</addressOffset>
                    <size>16</size>
                    <access>read-only</access>
                    <fields>
                        <field>
                            <name>LY</name>
                            <description>Current Scanline</description>
                            <bitRange>[7:0]</bitRange>
                            <access>read-only</access>
                        </field>
                    </fields>
                </register>
                <!-- BGCNT -->
                <register>
                    <dim>4</dim>
                    <dimIncrement>2</dimIncrement>
                    <dimIndex>0,1,2,3</dimIndex>
                    <name>BG%sCNT</name>
                    <description>BG%s Control</description>
                    <addressOffset>0x8</addressOffset>
                    <size>16</size>
                    <access>read-write</access>
                    <fields>
                        <field>
                            <name>Priority</name>
                            <description>Set Priority of this Background</description>
                            <bitRange>[1:0]</bitRange>
                            <access>read-write</access>
                            <enumeratedValues>
                                <enumeratedValue>
                                    <name>Highest</name>
                                    <value>0</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>High</name>
                                    <value>1</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>Normal</name>
                                    <value>2</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>Low</name>
                                    <value>3</value>
                                </enumeratedValue>
                            </enumeratedValues>
                        </field>
                        <field>
                            <name>CharacterBaseBlock</name>
                            <description>Set BG Tile date in units of 16KB</description>
                            <bitRange>[3:2]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>Mosaic</name>
                            <bitRange>[6:6]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>ColorsPalettes</name>
                            <description>Set Palettes or Color Types</description>
                            <bitRange>[7:7]</bitRange>
                            <access>read-write</access>
                            <enumeratedValues>
                                <enumeratedValue>
                                    <name>Limited</name>
                                    <description>16 colors in 16 arrays</description>
                                    <value>0</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>Full</name>
                                    <description>256 colors in one array</description>
                                    <value>1</value>
                                </enumeratedValue>
                            </enumeratedValues>
                        </field>
                        <field>
                            <name>ScreenBaseBlock</name>
                            <description>Set BG Map data in units of 2KB</description>
                            <bitRange>[12:8]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>DisplayAreaOverflow</name>
                            <description>How is Background displayed when Viewport reaches out this Background (only for Background 2/3)</description>
                            <bitRange>[13:13]</bitRange>
                            <access>read-write</access>
                            <enumeratedValues>
                                <enumeratedValue>
                                    <name>Transparent</name>
                                    <description>Background becomes transparent</description>
                                    <value>0</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>Wraparound</name>
                                    <description>Background will wrap around</description>
                                    <value>1</value>
                                </enumeratedValue>
                            </enumeratedValues>
                        </field>
                        <field>
                            <name>ScreenSize</name>
                            <description>Sets the Screen Size</description>
                            <bitRange>[15:14]</bitRange>
                            <access>read-write</access>
                            <enumeratedValues>
                                <enumeratedValue>
                                    <name>Small</name>
                                    <description>Text Mode: 256x256 (2K) | Rotation/Scaling Mode: 128x128 (256 bytes)</description>
                                    <value>0</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>Medium</name>
                                    <description>Text Mode: 512x256 (4K) | Rotation/Scaling Mode: 256x256 (1K)</description>
                                    <value>1</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>High</name>
                                    <description>Text Mode: 256x512 (4K) | Rotation/Scaling Mode: 512x512 (4K)</description>
                                    <value>2</value>
                                </enumeratedValue>
                                <enumeratedValue>
                                    <name>Large</name>
                                    <description>Text Mode: 512x512 (8K) | Rotation/Scaling Mode: 1024x1024 (16K)</description>
                                    <value>3</value>
                                </enumeratedValue>
                            </enumeratedValues>
                        </field>
                    </fields>
                </register>
                <!-- BGScroll -->
                <cluster>
                    <dim>4</dim>
                    <dimIncrement>0x4</dimIncrement>
                    <dimIndex>0,1,2,3</dimIndex>
                    <name>BG%sOFS</name>
                    <description>Set Background %s Offset</description>
                    <addressOffset>0x10</addressOffset>
                    <register>
                        <name>H</name>
                        <description>Horizontal Offset</description>
                        <addressOffset>0x0</addressOffset>
                        <size>16</size>
                        <access>write-only</access>
                        <fields>
                            <field>
                                <name>Offset</name>
                                <description>Set the Horizontal Offset of this Background</description>
                                <bitRange>[8:0]</bitRange>
                                <access>write-only</access>
                            </field>
                        </fields>
                    </register>
                    <register>
                        <name>V</name>
                        <description>Vertical Offset</description>
                        <addressOffset>0x2</addressOffset>
                        <size>16</size>
                        <access>write-only</access>
                        <fields>
                            <field>
                                <name>Offset</name>
                                <description>Set the Vertical Offset of this Background</description>
                                <bitRange>[8:0]</bitRange>
                                <access>write-only</access>
                            </field>
                        </fields>
                    </register>
                </cluster>
                <!-- BG Rotation Scaling -->
                <cluster>
                    <dim>2</dim>
                    <dimIncrement>0x10</dimIncrement>
                    <dimIndex>2,3</dimIndex>
                    <name>BG%sROTSCAL</name>
                    <description>Set Background %s Offset</description>
                    <addressOffset>0x20</addressOffset>
                    <register>
                        <name>PA</name>
                        <description>BG Rotation/Scaling Parameter A (alias dx)</description>
                        <addressOffset>0x0</addressOffset>
                        <size>16</size>
                        <access>write-only</access>
                        <fields>
                            <field>
                                <name>FractionalPortion</name>
                                <bitRange>[7:0]</bitRange>
                                <access>write-only</access>
                            </field>
                            <field>
                                <name>IntegerPortion</name>
                                <bitRange>[14:8]</bitRange>
                                <access>write-only</access>
                            </field>
                            <field>
                                <name>Sign</name>
                                <bitRange>[15:15]</bitRange>
                                <access>write-only</access>
                            </field>
                        </fields>
                    </register>
                    <register derivedFrom="PA">
                        <name>PB</name>
                        <description>BG Rotation/Scaling Parameter B (alias dmx)</description>
                        <addressOffset>0x2</addressOffset>
                    </register>
                    <register derivedFrom="PA">
                        <name>PC</name>
                        <description>BG Rotation/Scaling Parameter C (alias dy)</description>
                        <addressOffset>0x4</addressOffset>
                    </register>
                    <register derivedFrom="PA">
                        <name>PD</name>
                        <description>BG Rotation/Scaling Parameter D (alias dmy)</description>
                        <addressOffset>0x6</addressOffset>
                    </register>
                    <register>
                        <name>X</name>
                        <description>BG Reference Point X-Coordinate</description>
                        <addressOffset>0x8</addressOffset>
                        <size>32</size>
                        <access>write-only</access>
                        <fields>
                            <field>
                                <name>FractionalPortion</name>
                                <bitRange>[7:0]</bitRange>
                                <access>write-only</access>
                            </field>
                            <field>
                                <name>IntegerPortion</name>
                                <bitRange>[26:8]</bitRange>
                                <access>write-only</access>
                            </field>
                            <field>
                                <name>Sign</name>
                                <bitRange>[27:27]</bitRange>
                                <access>write-only</access>
                            </field>
                        </fields>
                    </register>
                    <register derivedFrom="X">
                        <name>Y</name>
                        <description>BG Reference Point Y-Coordinate</description>
                        <addressOffset>0xC</addressOffset>
                    </register>
                </cluster>
                <!-- WINH -->
                <register>
                    <dim>2</dim>
                    <dimIncrement>2</dimIncrement>
                    <dimIndex>0,1</dimIndex>
                    <name>WIN%sH</name>
                    <description>Window %s Horizontal Dimensions</description>
                    <addressOffset>0x40</addressOffset>
                    <size>16</size>
                    <access>write-only</access>
                    <fields>
                        <field>
                            <name>X2</name>
                            <description>Rightmost coordinate of window, plus 1</description>
                            <bitRange>[7:0]</bitRange>
                            <access>write-only</access>
                        </field>
                        <field>
                            <name>X1</name>
                            <description>Leftmost coordinate of window</description>
                            <bitRange>[15:8]</bitRange>
                            <access>write-only</access>
                        </field>
                    </fields>
                </register>
                <!-- WINV -->
                <register>
                    <dim>2</dim>
                    <dimIncrement>2</dimIncrement>
                    <dimIndex>0,1</dimIndex>
                    <name>WIN%sV</name>
                    <description>Window %s Vertical Dimensions</description>
                    <addressOffset>0x44</addressOffset>
                    <size>16</size>
                    <access>write-only</access>
                    <fields>
                        <field>
                            <name>Y2</name>
                            <description>Bottom-most coordinate of window, plus 1</description>
                            <bitRange>[7:0]</bitRange>
                            <access>write-only</access>
                        </field>
                        <field>
                            <name>Y1</name>
                            <description>Top-most coordinate of window</description>
                            <bitRange>[15:8]</bitRange>
                            <access>write-only</access>
                        </field>
                    </fields>
                </register>
                <!-- WININ -->
                <register>
                    <name>WININ</name>
                    <description>Control of Inside of Window(s)</description>
                    <addressOffset>0x48</addressOffset>
                    <size>16</size>
                    <access>read-write</access>
                    <fields>
                        <field>
                            <name>W0BG0Flag</name>
                            <description>Window 0 BG0 Enable Bit</description>
                            <bitRange>[0:0]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>W0BG1Flag</name>
                            <description>Window 0 BG1 Enable Bit</description>
                            <bitRange>[1:1]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>W0BG2Flag</name>
                            <description>Window 0 BG2 Enable Bit</description>
                            <bitRange>[2:2]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>W0BG3Flag</name>
                            <description>Window 0 BG3 Enable Bit</description>
                            <bitRange>[3:3]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>W0OBJFlag</name>
                            <description>Window 0 OBJ Enable Bit</description>
                            <bitRange>[4:4]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>W0ColorSFXFlag</name>
                            <description>Window 0 Color Special Effect</description>
                            <bitRange>[5:5]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>W1BG0Flag</name>
                            <description>Window 1 BG0 Enable Bit</description>
                            <bitRange>[8:8]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>W1BG1Flag</name>
                            <description>Window 1 BG1 Enable Bit</description>
                            <bitRange>[9:9]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>W1BG2Flag</name>
                            <description>Window 1 BG2 Enable Bit</description>
                            <bitRange>[10:10]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>W1BG3Flag</name>
                            <description>Window 1 BG3 Enable Bit</description>
                            <bitRange>[11:11]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>W1OBJFlag</name>
                            <description>Window 1 OBJ Enable Bit</description>
                            <bitRange>[12:12]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>W1ColorSFXFlag</name>
                            <description>Window 1 Color Special Effect</description>
                            <bitRange>[13:13]</bitRange>
                            <access>read-write</access>
                        </field>
                    </fields>
                </register>
                <!-- WINOUT -->
                <register>
                    <name>WINOUT</name>
                    <description>Control of Outside of Windows And Inside of OBJ Window</description>
                    <addressOffset>0x4A</addressOffset>
                    <size>16</size>
                    <access>read-write</access>
                    <fields>
                        <field>
                            <name>OutBG0Flag</name>
                            <description>Outside BG0 Enable Bit</description>
                            <bitRange>[0:0]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OutBG1Flag</name>
                            <description>Outside BG1 Enable Bit</description>
                            <bitRange>[1:1]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OutBG2Flag</name>
                            <description>Outside BG2 Enable Bit</description>
                            <bitRange>[2:2]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OutBG3Flag</name>
                            <description>Outside BG3 Enable Bit</description>
                            <bitRange>[3:3]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OutOBJFlag</name>
                            <description>Outside OBJ Enable Bit</description>
                            <bitRange>[4:4]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OutColorSFXFlag</name>
                            <description>Outside Color Special Effect</description>
                            <bitRange>[5:5]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OBJWBG0Flag</name>
                            <description>OBJ Window BG0 Enable Bit</description>
                            <bitRange>[8:8]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OBJWBG1Flag</name>
                            <description>OBJ Window BG1 Enable Bit</description>
                            <bitRange>[9:9]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OBJWBG2Flag</name>
                            <description>OBJ Window BG2 Enable Bit</description>
                            <bitRange>[10:10]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OBJWBG3Flag</name>
                            <description>OBJ Window BG3 Enable Bit</description>
                            <bitRange>[11:11]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OBJWOBJFlag</name>
                            <description>OBJ Window OBJ Enable Bit</description>
                            <bitRange>[12:12]</bitRange>
                            <access>read-write</access>
                        </field>
                        <field>
                            <name>OBJWColorSFXFlag</name>
                            <description>OBJ Window Color Special Effect</description>
                            <bitRange>[13:13]</bitRange>
                            <access>read-write</access>
                        </field>
                    </fields>
                </register>
                <!-- MOSAIC -->
                <register>
                    <name>MOSAIC</name>
                    <description>Mosaic Size</description>
                    <addressOffset>0x4C</addressOffset>
                    <size>16</size>
                    <access>write-only</access>
                    <fields>
                        <field>
                            <name>BGMosaicHSize</name>
                            <description>BG Mosaic H-Size (minus 1)</description>
                            <bitRange>[3:0]</bitRange>
                            <access>write-only</access>
                        </field>
                        <field>
                            <name>BGMosaicVSize</name>
                            <description>BG Mosaic V-Size (minus 1)</description>
                            <bitRange>[7:4]</bitRange>
                            <access>write-only</access>
                        </field>
                        <field>
                            <name>OBJMosaicHSize</name>
